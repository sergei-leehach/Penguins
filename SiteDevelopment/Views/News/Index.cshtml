@model IEnumerable<News>
@using System.ServiceModel.Channels
@using SiteDevelopment.Models;

@{
    ViewBag.Title = "News";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>
    All News of Pittsburgh Penguins
    <a href="@Url.Action("Create", "News")"><div class="button" style="float: right">Добавить новость</div></a>
</h1>


@foreach (var item in Model.Reverse())
{
    @Html.Partial("AddIndex", item)
}

